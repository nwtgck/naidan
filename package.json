{
  "name": "naidan",
  "private": true,
  "version": "0.14.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && npm run build:all",
    "build:all": "npm run build:standalone && npm run build:hosted",
    "build:standalone": "vite build --mode standalone --logLevel warn",
    "build:hosted": "vite build --mode hosted --logLevel warn",
    "preview": "vite preview",
    "test": "vitest",
    "test:only-failed": "vitest --reporter=./src/FailedOnlyReporter.ts --run",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@huggingface/transformers": "^4.0.0-next.3",
    "@vueuse/core": "^14.1.0",
    "comlink": "^4.4.2",
    "dompurify": "^3.3.1",
    "he": "^1.2.0",
    "highlight.js": "^11.11.1",
    "jszip": "^3.10.1",
    "katex": "^0.16.27",
    "lucide-vue-next": "^0.562.0",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.3",
    "marked-katex-extension": "^5.1.6",
    "mermaid": "^10.9.5",
    "nanoid": "^5.1.6",
    "unplugin-vue-router": "^0.19.2",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/dompurify": "^3.0.5",
    "@types/he": "^1.2.3",
    "@types/jsdom": "^27.0.0",
    "@types/katex": "^0.16.7",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.10.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "happy-dom": "^20.1.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "rollup-plugin-license": "^3.6.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.2.4",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-static-copy": "^3.1.6",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.4"
  }
